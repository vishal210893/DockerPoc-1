            <systemPath>${project.basedir}/lib/xmlgraphics-commons-2.1.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.solum</groupId>
            <artifactId>solum.batik-all</artifactId>
            <version>1.8</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/lib/batik-all-1.8.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.solum</groupId>
            <artifactId>solum.avalon-framework-api</artifactId>
            <version>4.3.1</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/lib/avalon-framework-api-4.3.1.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.solum</groupId>
            <artifactId>solum.avalon-framework-impl</artifactId>
