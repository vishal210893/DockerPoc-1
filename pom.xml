            <systemPath>${project.basedir}/lib/serializer-2.7.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.solum</groupId>
            <artifactId>solum.xalan</artifactId>
            <version>2.7.2</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/lib/xalan-2.7.2.jar</systemPath>
